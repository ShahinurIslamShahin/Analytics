------------------------------------------------------------------ONLY VOICE USER-------------------------------------------------------


-------------TOTAL--------------------
SELECT COUNT(MSISDN) FROM 
(SELECT /*+parallel(p,16)*/ DISTINCT V372_CALLINGPARTYNUMBER  MSISDN
FROM L3_VOICE P
WHERE V387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND V378_SERVICEFLOW=1
GROUP BY V372_CALLINGPARTYNUMBER) A
WHERE 
NOT EXISTS
(SELECT MSISDN1 
FROM
((
SELECT /*+parallel(p,16)*/ DISTINCT G372_CALLINGPARTYNUMBER MSISDN1
FROM L3_DATA P
WHERE G383_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
GROUP BY G372_CALLINGPARTYNUMBER)


UNION

(SELECT /*+parallel(p,16)*/ DISTINCT S372_CALLINGPARTYNUMBER
FROM L3_SMS P
WHERE S387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND S378_SERVICEFLOW=1
GROUP BY S372_CALLINGPARTYNUMBER)
)
WHERE MSISDN=MSISDN1
)


----------------------REPEAT-------------------------


SELECT COUNT(*) FROM 
(SELECT MSISDN FROM 
(SELECT /*+parallel(p,16)*/ DISTINCT V372_CALLINGPARTYNUMBER  MSISDN
FROM L3_VOICE P
WHERE V387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND V378_SERVICEFLOW=1
GROUP BY V372_CALLINGPARTYNUMBER) A
WHERE 
NOT EXISTS
(SELECT MSISDN1 
FROM
((
SELECT /*+parallel(p,16)*/ DISTINCT G372_CALLINGPARTYNUMBER MSISDN1
FROM L3_DATA P
WHERE G383_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
GROUP BY G372_CALLINGPARTYNUMBER)


UNION

(SELECT /*+parallel(p,16)*/ DISTINCT S372_CALLINGPARTYNUMBER
FROM L3_SMS P
WHERE S387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND S378_SERVICEFLOW=1
GROUP BY S372_CALLINGPARTYNUMBER)
)
WHERE MSISDN=MSISDN1
)
)P
WHERE 
EXISTS
(
select V372_CALLINGPARTYNUMBER1  from
(SELECT V372_CALLINGPARTYNUMBER V372_CALLINGPARTYNUMBER1 ,TOTAL,DISTINCT_CALLEDPARTY FROM (
SELECT /*+parallel(p,16)*/ DISTINCT V372_CALLINGPARTYNUMBER, COUNT(V373_CALLEDPARTYNUMBER) AS TOTAL, COUNT(DISTINCT V373_CALLEDPARTYNUMBER) AS DISTINCT_CALLEDPARTY 
FROM L3_VOICE P
WHERE V387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND V378_SERVICEFLOW=1
GROUP BY V372_CALLINGPARTYNUMBER)
WHERE (TOTAL/DISTINCT_CALLEDPARTY)>1
)Q
WHERE V372_CALLINGPARTYNUMBER1=MSISDN
)




--------------------UNIQUE--------------------------



SELECT COUNT(*) FROM 
(SELECT MSISDN FROM 
(SELECT /*+parallel(p,16)*/ DISTINCT V372_CALLINGPARTYNUMBER  MSISDN
FROM L3_VOICE P
WHERE V387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND V378_SERVICEFLOW=1
GROUP BY V372_CALLINGPARTYNUMBER) A
WHERE 
NOT EXISTS
(SELECT MSISDN1 
FROM
((
SELECT /*+parallel(p,16)*/ DISTINCT G372_CALLINGPARTYNUMBER MSISDN1
FROM L3_DATA P
WHERE G383_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
GROUP BY G372_CALLINGPARTYNUMBER)


UNION

(SELECT /*+parallel(p,16)*/ DISTINCT S372_CALLINGPARTYNUMBER
FROM L3_SMS P
WHERE S387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND S378_SERVICEFLOW=1
GROUP BY S372_CALLINGPARTYNUMBER)
)
WHERE MSISDN=MSISDN1
)
)P
WHERE 
EXISTS
(
select V372_CALLINGPARTYNUMBER1  from
(SELECT V372_CALLINGPARTYNUMBER V372_CALLINGPARTYNUMBER1 ,TOTAL,DISTINCT_CALLEDPARTY FROM (
SELECT /*+parallel(p,16)*/ DISTINCT V372_CALLINGPARTYNUMBER, COUNT(V373_CALLEDPARTYNUMBER) AS TOTAL, COUNT(DISTINCT V373_CALLEDPARTYNUMBER) AS DISTINCT_CALLEDPARTY 
FROM L3_VOICE P
WHERE V387_CHARGINGTIME_KEY=(SELECT DATE_KEY FROM DATE_DIM WHERE DATE_VALUE=TO_DATE('23/02/2021','dd/mm/rrrr'))
AND V378_SERVICEFLOW=1
GROUP BY V372_CALLINGPARTYNUMBER)
WHERE (TOTAL/DISTINCT_CALLEDPARTY)=1
)Q
WHERE V372_CALLINGPARTYNUMBER1=MSISDN
)



